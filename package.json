{
  "name": "piral-service-nodejs-mongo-s3-azure",
  "version": "1.0.0",
  "description": "A simple Node.js sample pilet feed service for use with Piral. ðŸš€",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "npx nodemon",
    "start": "npm run build && node build/index.js",
    "lint": "eslint . --ext .ts",
    "prettier-format": "run-script-os",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "husky install",
    "prisma:migrate": "npx prisma migrate dev --name init && npx prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:prisma:migrate": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test": "yarn docker:up && yarn test:prisma:migrate && dotenv -e .env.test -- jest -i",
    "test:dev": "yarn docker:up && yarn test:prisma:migrate && dotenv -e .env.test -- jest --watchAll",
    "start:docker": "npm run prisma:migrate && npm run prisma:seed && node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leandrovitto/piral-service-nodejs-mongo-s3-azure.git"
  },
  "keywords": [
    "piral",
    "micro-frontend",
    "piral-service"
  ],
  "author": "Leandro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leandrovitto/piral-service-nodejs-mongo-s3-azure/issues"
  },
  "homepage": "https://github.com/leandrovitto/piral-service-nodejs-mongo-s3-azure#readme",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.1",
    "@types/response-time": "^2.3.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/tar": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "axios": "^1.3.4",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^2.0.21",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "prisma": "^4.11.0",
    "rimraf": "^4.4.0",
    "run-script-os": "^1.1.6",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.298.0",
    "@aws-sdk/lib-storage": "^3.298.0",
    "@azure/storage-blob": "^12.13.0",
    "@prisma/client": "^4.11.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest-cucumber": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "liquidjs": "^10.7.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "response-time": "^2.3.2",
    "swagger-ui-express": "^4.6.2",
    "tar": "^6.1.13",
    "uuid": "^9.0.0"
  }
